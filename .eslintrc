{
    "parser": "babel-eslint",
    "env": {
        "node": true
    },
    "settings": {
        "ecmascript": 5
    },
    "rules": {
        "strict": 0,
        "quotes": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "curly": 0,
        "consistent-return": 0,
        "key-spacing": 0,
        "no-alert": 0,
        "new-cap": 0,
        "no-use-before-define": [1, "nofunc"],
        "no-mixed-requires": 0,
        "no-empty": 0
    }
}
