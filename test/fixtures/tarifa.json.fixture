  "cordova": {
    "preferences": {
      "DisallowOverscroll": true,
      "EnableViewportScale": false,
      "KeyboardDisplayRequiresUserAction": false,
      "ShowSplashScreenSpinner": false,
      "SplashScreen": "screen",
      "BackgroundColor": "0xffffffff",
      "AutoHideSplashScreen": false,
      "KeepRunning": true,
      "Orientation": "portrait"
    },
    "whitelist": {
      "shared": [
        {
          "type": "access-origin",
          "origin": [
            "*"
          ]
        },
        {
          "type": "allow-intent",
          "origin": [
            "http://*/*",
            "https://*/*",
            "tel:*",
            "sms:*",
            "mailto:*",
            "geo:*"
          ]
        }
      ],
      "android": [
        {
          "type": "allow-intent",
          "origin": [
            "market:*"
          ]
        },
        {
          "type": "allow-navigation",
          "origin": [
            "*"
          ]
        }
      ],
      "ios": [
        {
          "type": "allow-intent",
          "origin": [
            "itms:*",
            "itms-apps:*"
          ]
        }
      ]
    }
  },
  "author": {
